{
    "name": "sebastiaanluca/laravel-skeleton",
    "description": "Laravel skeleton application",
    "keywords": [
        "framework",
        "laravel",
        "skeleton",
        "scaffolding"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.5",
        "graham-campbell/exceptions": "^9.3",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "laravelcollective/html": "^5.4",
        "nesbot/carbon": "^1.22",
        "nwidart/laravel-modules": "^1.16",
        "predis/predis": "^1.1",
        "sebastiaanluca/laravel-conditional-providers": "^0.1",
        "sebastiaanluca/laravel-helpers": "0.*",
        "sebastiaanluca/laravel-router": "^2.1",
        "sebastiaanluca/laravel-unbreakable-migrations": "^0.1",
        "sebastiaanluca/laravel-validator": "^0.1",
        "spatie/laravel-backup": "^4.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.2",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.6",
        "hannesvdvreken/guzzle-debugbar": "^2",
        "laravel/envoy": "^1.3",
        "laravel/homestead": "^5",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "^6",
        "raveren/kint": "^1",
        "spatie/laravel-db-snapshots": "^1.0",
        "symfony/css-selector": "^3.2",
        "symfony/dom-crawler": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "composer dumpautoload -o",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "composer dumpautoload",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --nowrite",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
